    <dubbo:reference interface="com.wuxiu.galaxy.api.service.LuggageTypeFacade"
                     id="luggageTypeFacade"
                     version="${dubbo.consumer.version}"
                     timeout="${dubbo.consumer.timeout}"/>

    <dubbo:reference interface="com.wuxiu.galaxy.api.service.PickupLuggageFacade"
                     id="pickupLuggageFacade"
                     version="${dubbo.consumer.version}"
                     timeout="${dubbo.consumer.timeout}"/>

    <dubbo:reference interface="com.wuxiu.galaxy.api.service.TurnoverRecordFacade"
                     id="turnoverRecordFacade"
                     version="${dubbo.consumer.version}"
                     timeout="${dubbo.consumer.timeout}"/>
