                     version="${dubbo.consumer.version}"
                     timeout="${dubbo.consumer.timeout}"/>

    <dubbo:reference interface="com.wuxiu.galaxy.api.service.ChargeCalculateRuleFacade"
                     id="chargeCalculateRuleFacade"
                     version="${dubbo.consumer.version}"
                     timeout="${dubbo.consumer.timeout}"/>

    <dubbo:reference interface="com.wuxiu.galaxy.api.service.LuggageLostCompensateFacade"
                     id="luggageLostCompensateFacade"
                     version="${dubbo.consumer.version}"
